		System.out.print("Enter Password:");
		String newPass=sc.nextLine();
		
		for(int i=0;i<)
		
		nname[count]=newName;
		pass[count]=newPass;
		count++;
		
		System.out.println("Successfully Registered....");
		
		
	}
	
	public static void main(String[] args) {
		int num;
		
