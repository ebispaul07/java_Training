package com.stri;

public class OccurenceOfChar {
	public static void main(String[] args) {
		String s="Hello@";
		
		int a[]=new int[256];
		
		for(int i=0;i<s.length();i++) {
			a[s.charAt(i)]++;
		}
		
		
		for(int i=0;i<a.length;i++) {
			
			if(a[i]>0) {
			System.out.println((char)i+" "+a[i]);
			}
		}
		
	}

}
