package com.excep;

public class SampleCheck {
	public static void main(String[] args) {
		try {
			int a[]= {1,2,3,4};
			System.out.println(a[8]);
			
		}catch(ArrayIndexOutOfBoundsException e) {
			System.out.println(e.getMessage());
		}catch(Exception e) {
			System.out.println(e.getMessage());
		}
	}

}
